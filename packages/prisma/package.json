{
  "name": "@cms-app/prisma",
  "version": "1.0.0",
  "description": "Centralized Prisma client and types for CMS application",
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "generate": "prisma generate && npm run build",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@prisma/client": "^6.10.1",
    "prisma": "^6.10.1"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "require": "./index.js",
      "default": "./index.js"
    },
    "./browser-types": {
      "types": "./browser-types.d.ts",
      "import": "./browser-types.js",
      "require": "./browser-types.js",
      "default": "./browser-types.js"
    },
    "./client": {
      "types": "./generated/client/index.d.ts",
      "import": "./generated/client/index.js",
      "require": "./generated/client/index.js",
      "default": "./generated/client/index.js"
    },
    "./generated/client": {
      "types": "./generated/client/index.d.ts",
      "import": "./generated/client/index.js",
      "require": "./generated/client/index.js",
      "default": "./generated/client/index.js"
    }
  }
}
