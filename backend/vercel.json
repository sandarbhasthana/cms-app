{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts"
    }
  ],
  "buildCommand": "cd ../packages/prisma && npm run generate && cd ../backend && npm run build",
  "installCommand": "npm install",
  "env": {
    "NODE_ENV": "production"
  }
}
